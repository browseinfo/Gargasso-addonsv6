<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    	
		<record id="awg_parameters_form_view" model="ir.ui.view">
            <field name="name">awg.parameters.form</field>
            <field name="model">awg.parameters</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            	<form string="Product AWG Parameters">
                	<field name="awg_code" colspan="4" select="2"/>
                	<field name="name" colspan="4" nolabel="1"/>
                </form>
            </field>
        </record>

		<record id="awg_parameters_tree_view" model="ir.ui.view">
            <field name="name">awg.parameters.tree</field>
            <field name="model">awg.parameters</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
            	<tree string="Quality Testing">
                	<field name="awg_code"/>
                	<field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>
